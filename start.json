{
  "apps": [{
    "name": "react-blog",
    "script": "./server/app.js",
    "exec_mode": "cluster",
    "env_dev": {
      "NODE_ENV": "development"
    },
    "env_production": {
       "NODE_ENV": "production"
    },
    "merge_logs": true,
    "out_file": "./logs/out.log", 
    "error_file": "./logs/error.log", 
    "instances":4,
    "autorestart": true
  }]
}

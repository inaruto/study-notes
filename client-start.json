{
  "apps": [{
    "name": "react-blog-scripts",
    "script": "./scripts/start.js",
    "exec_mode": "cluster",
    "env": {
      "NODE_ENV": "development"
    },
    "env_production": {
      "NODE_ENV": "production"
    },
    "merge_logs": true,
    "out_file": "./logs/client-out.log", 
    "error_file": "./logs/client-error.log", 
    "instances":1,
    "autorestart": true
  }]
}
